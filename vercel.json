{
  "rewrites": [
    { "source": "/tracker.js", "destination": "/tracker/tracker.js" },
    { "source": "/consent-banner.js", "destination": "/tracker/consent-banner.js" },
    { "source": "/api/:path*", "destination": "/api/index.js" },
    { "source": "/login", "destination": "/dashboard/login.html" },
    { "source": "/login.html", "destination": "/dashboard/login.html" },
    { "source": "/css/:path*", "destination": "/dashboard/css/:path*" },
    { "source": "/js/:path*", "destination": "/dashboard/js/:path*" },
    { "source": "/", "destination": "/dashboard/index.html" }
  ],
  "headers": [
    {
      "source": "/tracker.js",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    },
    {
      "source": "/api/:path*",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST,PUT,DELETE,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
    }
  ]
}
