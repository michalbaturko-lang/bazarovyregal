{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "dashboard/**",
      "use": "@vercel/static"
    },
    {
      "src": "tracker/tracker.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/tracker.js",
      "dest": "/tracker/tracker.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/css/(.*)",
      "dest": "/dashboard/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/dashboard/js/$1"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/dashboard/$1"
    },
    {
      "src": "/",
      "dest": "/dashboard/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/dashboard/$1"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase-url",
    "SUPABASE_SERVICE_KEY": "@supabase-service-key"
  }
}
